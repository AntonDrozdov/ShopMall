@{
    Layout = "_AdminShopLayout";
}

<h2>Goods</h2>

<!--КНОПКА СОЗДАНИЯ ПОЗИЦИИ-->
<a class="btn btn-primary" role="button" asp-controller="Manage" asp-action="CreateGood" >Новая позиция</a>
<!--<a asp-controller="Manage" asp-action="CreateGood">Новая позиция</a>-->

<ul>
    @foreach (var good in ViewBag.Goods)
    {
        <li>@good.Title</li>
    }
</ul>